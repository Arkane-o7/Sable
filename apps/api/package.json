{
  "name": "@sable/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/index.ts --format esm --dts",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
