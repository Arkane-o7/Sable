    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@hono/zod-validator": "^0.4.1",
    "@neondatabase/serverless": "^0.10.4",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.34.3",
    "@workos-inc/node": "^7.36.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.38.3",
    "groq-sdk": "^0.3.2",
    "hono": "^4.6.14",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "drizzle-kit": "^0.30.1",
    "tsup": "^8.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3"
  }
