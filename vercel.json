{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "buildCommand": "echo 'skip build'",
  "outputDirectory": ".",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.2.29"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/index.ts" }
  ]
}
